{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": "off",
        // Indent with 4 spaces
        "indent": [
            "error",
            4, 8, 12
        ]
    }
}
